<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Tigren_Ajaxsuite::ajaxsuite_main" title="Ajaxsuite" sortOrder="20">
                    <resource id="Tigren_Ajaxsuite::ajaxsuite" title="Ajaxsuite" sortOrder="1"></resource>
                    <resource id="Tigren_Ajaxsuite::ajaxlogin" title="Ajaxlogin" sortOrder="2"></resource>
                    <resource id="Tigren_Ajaxsuite::ajaxcompare" title="Ajaxcompare" sortOrder="3"></resource>
